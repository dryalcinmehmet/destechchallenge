          { "expr": "sum(celery_task_received_total)", "refId": "A" }
        ],
        "gridPos": { "x": 6, "y": 16, "w": 6, "h": 4 }
      },
  
      {
        "type": "timeseries",
        "title": "Celery Success / Failed Trend",
        "targets": [
          { "expr": "sum(celery_task_succeeded_total)", "refId": "A" },
          { "expr": "sum(celery_task_failed_total)", "refId": "B" }
        ],
        "gridPos": { "x": 0, "y": 20, "w": 12, "h": 6 }
