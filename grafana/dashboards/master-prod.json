{
    "annotations": {
      "list": []
    },
    "editable": true,
    "title": "Destech - Uygulama İzleme Dashboard PRODUCTION",
    "timezone": "browser",
    "schemaVersion": 36,
    "version": 7,
    "panels": [
      {
        "type": "stat",
        "title": "Toplam HTTP İstek Sayısı",
        "targets": [
          { "expr": "django_http_requests_before_middlewares_total", "refId": "A" }
        ],
        "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 }
      },
      {
        "type": "stat",
        "title": "Toplam HTTP Response Sayısı",
        "targets": [
          { "expr": "django_http_responses_before_middlewares_total", "refId": "A" }
        ],
        "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 }
      },
  
      {
        "type": "timeseries",
        "title": "Ortalama Request Yanıt Süresi (ms)",
        "targets": [
          {
            "expr": "1000 * (django_http_requests_latency_including_middlewares_seconds_sum / django_http_requests_latency_including_middlewares_seconds_count)",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 4, "w": 12, "h": 6 }
      },
  
      {
        "type": "heatmap",
        "title": "Request Latency Histogramı",
        "targets": [
          { "expr": "django_http_requests_latency_including_middlewares_seconds_bucket", "refId": "A" }
        ],
        "gridPos": { "x": 0, "y": 10, "w": 12, "h": 6 }
      },
  
      {
        "type": "stat",
        "title": "Celery Gönderilen Task Sayısı",
        "targets": [
          { "expr": "sum(celery_task_sent_total)", "refId": "A" }
        ],
        "gridPos": { "x": 0, "y": 16, "w": 6, "h": 4 }
      },
      {
        "type": "stat",
